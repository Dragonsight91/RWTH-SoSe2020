class NFA(object):
    def __init__(self, Q: set):
        self.Q = Q
        self.delta = dict()

        for q in Q:
            self.delta.update({q: dict()})

    def addTransition(q:any, a:any, p:any) -> None:
        rho = self.delta.get(p)
        target = rho.get(a)
        if target:
            target.add(p)
        else:
            target = set()
            target.add(p)
            rho.update({a:target})

